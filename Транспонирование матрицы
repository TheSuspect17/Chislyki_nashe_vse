import numpy as np
import sympy
import time

# проверка на линейно-завимую матрицу
def combs(mat):
    temp = []
    _, inds = sympy.Matrix(mat).T.rref()
    inds = list(inds)
    for i in range (len(inds)):
        for j in range (len(mat)):
            if j == inds[i]:
                temp.append(mat[j])
    return temp

def trsp(mtrx):
  tr_mtrx = [[0 for j in range(len(mtrx)]for i in range (len(mtrx[0]))]
  for i in range (len(mtrx)):
    for j in range (len(mtrx[0])):
      tr_mtrx[j][i] = mtrx[i][j]
      tr_mtrx = combs(tr_mtrx)        
  return tr_mtrx
